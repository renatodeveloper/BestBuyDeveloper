        CREATE TABLE Usuario (idUsuario INTEGER PRIMARY KEY AUTOINCREMENT, dsNome TEXT, dsLogin TEXT, dsSenha TEXT, namePhoto TEXT, bytePhoto Blob);
    </string>

    <string name="sqlCreateTableCarro">
        CREATE TABLE Carro (id INTEGER PRIMARY KEY AUTOINCREMENT, nome TEXT, descricao TEXT, marca TEXT, quantidade INTEGER, preco FLOAT, imagem TEXT);
    </string>

    <string name="sqlCreateTablePedio">
        CREATE TABLE Pedido (idPedido INTEGER PRIMARY KEY AUTOINCREMENT, dataHora INTEGER, dsDataHora TEXT, total DOUBLE, idUsuario INTEGER, FOREIGN KEY (idUsuario) REFERENCES Usuario(idUsuario));
    </string>

    <string name="sqlCreateTableItemPedido">
        CREATE TABLE ItemPedido (idPedido INTEGER, idCarro INTEGER, precoUnitario DOUBLE, quantidade INTEGER, precoTotal DOUBLE, dsPojoToGson TEXT, PRIMARY KEY (idPedido, idCarro));
    </string>


